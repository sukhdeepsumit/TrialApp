<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trialapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TeacherActivity.SubjectList"></activity>
        <activity android:name=".TeacherActivity.MyCourseList" />
        <activity android:name=".StudentActivity.LogInStudentActivity" />
        <activity android:name=".TeacherActivity.SignUpTeacherActivity" />
        <activity android:name=".StudentActivity.SignUpStudentActivity" />
        <activity android:name=".TeacherActivity.LogInTeacherActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>